{
  "name": "Matrix Clipper",
  "version": "0.0.2.10",
  "description": "",
  "permissions": [
    "activeTab",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "optional_permissions": [],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["dhdmodal.js", "content.js"],
      "css": ["css/popup.css"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "24": "images/icon24.png",
    "32": "images/icon32.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "assets/*"
  ]
}
